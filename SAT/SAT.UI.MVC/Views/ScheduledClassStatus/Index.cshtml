@model IEnumerable<SAT.DATA.EF.Models.ScheduledClassStatus>

@{
    ViewData["Title"] = "Index";
}

<div class="container mt-5">
<h1>@ViewData["Title"]</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Scsname)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Scsname)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Scsid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Scsid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Scsid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>